    "speaker": "Suegra",
    "conditionalTexts": [
      { "requiredFlags": ["S1.1", "S1.2"], "text": "Tan agradable como siempre, no sé como Verónica te aguanta" },
      { "requiredFlags": ["S2.1"], "text": "Lo sé lo sé, ¿tienes algún plan para que deje de serlo? ¿o piensas quedarte con el taxi toda la vida como tu padre?" },
      { "requiredFlags": ["S2.2"], "text": "Formar una familia a mí me parece una cosa magnífica que se puede hacer en esta isla, es lo que hice yo" }
    ],
    "choices": [
      {
        "text": "Ella ha entendido que no hace falta hablar si no tienes nada que decir",
        "next": "3Suegra",
        "requiredFlags": ["S1.1"],
        "setFlags": ["S1.1.1"]
      },
      {
        "text": "Yo tampoco, para qué mentir",
